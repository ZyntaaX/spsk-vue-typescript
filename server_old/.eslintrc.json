{
  "env": {
      "browser": true,
      "es2021": true
  },
  "extends": "standard-with-typescript",
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "@typescript-eslint/prefer-nullish-coalescing": ["warn", {
          "ignoreConditionalTests": true,
          "ignoreMixedLogicalExpressions": true
        }],
        "quote-props": ["warn", "consistent"]
  }
}
